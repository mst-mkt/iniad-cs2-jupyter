    "def insertion_sort(l: list[int]) -> list[int]:\n",
    "    lst = l.copy()\n",
    "    for i in range(1, len(lst)):\n",
    "        j = i\n",
    "        while j > 0 and lst[j - 1] > lst[j]:\n",
    "            lst[j], lst[j - 1] = lst[j - 1], lst[j]\n",
    "            j -= 1\n",
    "    return lst"
   ]
  }
