    "result: list[int] = []\n",
    "def fib(n: int) -> int:\n",
    "    result.append(n)\n",
    "    if n < 2:\n",
    "        return max(n, 0)\n",
    "    return fib(n - 1) + fib(n - 2)\n"
   ]
